{
  "name": "awwbot",
  "version": "1.0.0",
  "description": "A simple discord bot that uses intents to post pictures from r/aww",
  "type": "module",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "start": "wrangler dev",
    "ngrok": "ngrok http 8787",
    "fix": "eslint --fix '**/*.{js,ts}'",
    "lint": "eslint '**/*.{js,ts}'",
    "register": "node dist/register.js",
    "publish": "wrangler deploy",
    "build": "rolldown -c rolldown.config.js",
    "dev": "rolldown -c rolldown.config.js --watch"
  },
  "keywords": [],
  "author": "Justin Beckwith <justin.beckwith@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "discord-interactions": "^4.0.0",
    "itty-router": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^16.0.0",
    "ngrok": "^5.0.0-beta.2",
    "prettier": "^3.2.5",
    "rolldown": "^1.0.0-beta.34",
    "sinon": "^21.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "wrangler": "^4.13.2"
  }
