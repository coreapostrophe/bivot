{
  "name": "bivot",
  "version": "1.0.0",
  "description": "The Bivo Bot",
  "type": "module",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "start": "wrangler dev",
    "prestart": "npm run types",
    "ngrok": "ngrok http 8787",
    "fix": "eslint --fix '**/*.{js,ts}'",
    "lint": "eslint '**/*.{js,ts}'",
    "register:dev": "node dist/register.js --env dev",
    "register:prod": "node dist/register.js --env prod",
    "preregister:dev": "npm run build",
    "preregister:prod": "npm run build",
    "publish": "wrangler deploy",
    "prepublish": "npm run build",
    "build": "rolldown -c rolldown.config.js",
    "dev": "wrangler dev & rolldown -c rolldown.config.js --watch",
    "predev": "npm run types",
    "types": "wrangler types ./src/worker-configuration.d.ts"
  },
  "keywords": [],
  "author": "Bivo",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.11.3",
    "@discordjs/rest": "^2.6.0",
    "discord-api-types": "^0.38.22",
    "discord-interactions": "^4.0.0",
    "itty-router": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^16.0.0",
    "ngrok": "^5.0.0-beta.2",
    "prettier": "^3.2.5",
    "rolldown": "^1.0.0-beta.34",
    "sinon": "^21.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "wrangler": "^4.36.0"
  }
}
